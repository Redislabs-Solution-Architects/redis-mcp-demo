{
  "mcpServers": {
    "redis": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--pull=always",
        "mcp/redis:latest"
      ],
      "env": {
        "REDIS_HOST": "YOUR_REDIS_HOST",
        "REDIS_PORT": "YOUR_REDIS_PORT",
        "REDIS_USERNAME": "default",
        "REDIS_PASSWORD": "YOUR_REDIS_PASSWORD"
      }
    },
    "redis-cloud": {
      "command": "npx",
      "args": [
        "-y",
        "@joshuakgoldberg/mcp-redis-cloud"
      ],
      "env": {
        "REDIS_CLOUD_API_KEY": "YOUR_API_KEY",
        "REDIS_CLOUD_API_SECRET": "YOUR_API_SECRET"
      }
    }
  }
}